# Plutus Frontend Environment Configuration
# Copy this file to .env.local and update the values

# Layercode Pipeline Configuration (REQUIRED)
# Get this from your Layercode Dashboard pipeline URL
NEXT_PUBLIC_LAYERCODE_PIPELINE_ID=your_pipeline_id_here

# Backend API Configuration (OPTIONAL)
# URL where your plutus-server is running
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000

# Development Configuration (OPTIONAL)
# Uncomment for additional debug information
# NEXT_PUBLIC_DEBUG=true 