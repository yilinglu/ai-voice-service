# CDK Environment Configuration
# Copy this file to .env and update the values

# AWS Configuration
CDK_DEFAULT_ACCOUNT=your-aws-account-id
CDK_DEFAULT_REGION=us-east-1

# Environment
ENVIRONMENT=dev

# GitHub Configuration (for CI/CD pipeline - optional)
GITHUB_OWNER=your-github-username
GITHUB_REPO=ai-voice-service
GITHUB_BRANCH=main
GITHUB_TOKEN_SECRET_NAME=github-token-secret-name

# Application Configuration
NEXT_PUBLIC_LAYERCODE_PIPELINE_ID=your-layercode-pipeline-id 